    <link href="~/Content/Inspenia/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Inspenia/font-awesome/css/font-awesome.css" rel="stylesheet" />

    <!-- Toastr style -->
    <link href="../Content/Inspenia/css/plugins/toastr/toastr.min.css" rel="stylesheet" />

    <!-- Gritter -->
    <link href="~/Content/Inspenia/js/plugins/gritter/jquery.gritter.css" rel="stylesheet" />
    <link href="~/Content/Inspenia/css/animate.css" rel="stylesheet" />
    <link href="~/Content/Inspenia/css/style.css" rel="stylesheet" />
</head>

<body>
    <div id="wrapper">
        <!-- Menu bar start -->
        <nav class="navbar-default navbar-static-side" role="navigation">
            <div class="sidebar-collapse">
                <ul class="nav metismenu" id="side-menu">
                    <li class="nav-header">
                        <div class="dropdown profile-element">
                            <asp:Image alt="image" ID="imgProfile" runat="server" class="white-bg img-circle" Width="120px" Height="120px" ImageUrl="~/Images/profile-male-icon.png" />
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                                <span class="clear">
                                    <span class="block m-t-xs">
                                        <strong class="font-bold">
                                            <asp:Label ID="lblUserName" runat="server" Text="User Name"></asp:Label>
                                        </strong>
                                        <br />
                                        <asp:Label ID="lblRole" runat="server" Text="Role"></asp:Label>
                                    </span>
                                </span>
                            </a>
                            <ul class="dropdown-menu animated fadeInRight m-t-xs">
                                <li><a class="dropdown-item" href="#">Profile</a></li>
                                <li><a class="dropdown-item" href="#">Change Password</a></li>
                                <li class="dropdown-divider"></li>
                                <li><a class="dropdown-item" href="#">Logout</a></li>
                            </ul>
                        </div>
                        <div class="logo-element">
                            IN+
                        </div>
                    </li>
                    <ul class="nav metismenu" id="ul1" runat="server">
                    </ul>
                </ul>

            </div>
        </nav>
        <!-- Menu bar end -->

        <div id="page-wrapper" class="gray-bg dashbard-1">
            <!-- Header start -->
            <div class="row">
                <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i></a>
                    </div>
                    <div class="navbar-header">
                        <p style="margin-left:30px; font-weight:bold; margin-top:12px; font-size:25px;">
                            Heading
                        </p>
                    </div>
                </nav>

                <!-- Main Content Start -->
                @RenderBody()

                <!-- Main Content end -->
            </div>
            <!-- Header end -->
            <!-- Footer start -->
            <div class="footer fixed">
                <div class="pull-right">
                    <div class="text-info"><strong>KTS</strong></div>
                </div>
                <div>
                    &copy; 2022 - KIOSK - <a asp-area="" asp-page="/Privacy">Privacy</a>
                </div>
            </div>
            <!-- Footer end -->
        </div>
    </div>

